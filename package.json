{
  "name": "omnio",
  "version": "0.0.0",
  "license": "MIT",
  "description": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./logger": {
      "types": "./dist/logger/index.d.ts",
      "default": "./dist/logger/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "default": "./dist/schemas.js"
    },
    "./db/node": {
      "types": "./dist/db/node-db.d.ts",
      "default": "./dist/db/node-db.js"
    },
    "./db/wasm": {
      "types": "./dist/db/wasm-db.d.ts",
      "default": "./dist/db/wasm-db.js"
    },
    "./fs/node": {
      "types": "./dist/fs/node-fs.d.ts",
      "default": "./dist/fs/node-fs.js"
    },
    "./fs/opfs": {
      "types": "./dist/fs/opfs.d.ts",
      "default": "./dist/fs/opfs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "LICENSE"
  ],
  "homepage": "https://github.com/tai-kun/omnio",
  "repository": {
    "url": "https://github.com/tai-kun/omnio"
  },
  "scripts": {
    "update": "run-s update:*",
    "update:nodejs": "volta pin node@lts",
    "update:module": "ncu --target minor --reject '@duckdb/duckdb-wasm' -u",
    "update:dprint": "dprint config update",
    "update:install": "run-s update:install:*",
    "update:install:packages": "npm install",
    "update:install:playwright": "playwright install --with-deps",
    "test": "run-s test:*",
    "test:server": "vitest --config .config/vitest.server.ts --run tests/",
    "test:client": "vitest --config .config/vitest.client.ts --run tests/",
    "test:typecheck": "tsc --noEmit",
    "build": "tsc -p .config/tsconfig.build.json"
  },
  "dependencies": {
    "hash-wasm": "^4.12.0",
    "mime": "^4.1.0",
    "normalize-url": "^8.1.0",
    "sql-template-tag": "^5.2.1",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@duckdb/node-api": "^1.3.4-alpha.27",
    "valibot": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.18.6",
    "@vitest/browser": "^3.2.4",
    "dprint": "^0.50.2",
    "esbuild": "^0.25.10",
    "esbuild-plugin-replace": "^1.4.0",
    "npm-check-updates": "^18.3.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.55.0",
    "superjson": "^2.2.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.19.0"
  }
}
